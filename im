[0;34m[0m        [0;32mif[0m [0mhue[0m [0;32mis[0m [0;32mNone[0m[0;34m:[0m[0;34m[0m
[0;34m[0m            [0mdiag_kind[0m [0;34m=[0m [0;34m"kde"[0m [0;32mif[0m [0mkind[0m [0;34m==[0m [0;34m"kde"[0m [0;32melse[0m [0;34m"hist"[0m[0;34m[0m
[0;34m[0m        [0;32melse[0m[0;34m:[0m[0;34m[0m
[0;34m[0m            [0mdiag_kind[0m [0;34m=[0m [0;34m"hist"[0m [0;32mif[0m [0mkind[0m [0;34m==[0m [0;34m"hist"[0m [0;32melse[0m [0;34m"kde"[0m[0;34m[0m
